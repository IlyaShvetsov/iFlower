<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_profile">

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.iflower.search.SearchMainFragment"
        android:label="@string/title_catalog"
        tools:layout="@layout/fragment_main_search" />

    <fragment
        android:id="@+id/navigation_photo"
        android:name="com.iflower.photo.PhotoMainFragment"
        android:label="@string/title_photo"
        tools:layout="@layout/fragment_main_photo" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.iflower.account.AccountFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_login" />

</navigation>